<script type="text/javascript">
    /*zee*/
    (function () {
        /**
         * Shorten a post
         * @param {string} input; the string to shorten
         * @param {Number} value; the breakpoint of the string
         * @param {boolean} end; Not mandatory, if true it will return the end of the string instead of the start
         * @return {string} shortenPost
         */
        function shortenPost(input, value, end) {
            const spacePosition = input.indexOf(' ', value);

            if (spacePosition === -1) {
                if (end) {
                    return false;
                }
                return input;
            }

            if (!end) {
                return input.substring(0, spacePosition);
            }
            return input.substring(spacePosition);
        }

        function shortenPostValue(windowWidth) {
            // @thib : use this default value for product description
            if (!windowWidth) return 150;

            if (windowWidth < 481) {
                return 80
            } else if (windowWidth < 768) {
                return 160
            } else if (windowWidth < 992) {
                return 300
            } else {
                return 600
            }
        }

        function doScrolling(elementY, duration) {
            const startingY = window.pageYOffset;
            const diff = elementY - startingY;
            let start;

            window.requestAnimationFrame(function step(timestamp) {
                if (!start) start = timestamp;
                const time = timestamp - start;
                const percent = Math.min(time / duration, 1);

                window.scrollTo(0, startingY + diff * percent);

                if (time < duration) {
                    window.requestAnimationFrame(step)
                }
            })
        }

        const authorDescriptionsElements =
            Array.prototype.slice.call(document.getElementsByClassName('category-description-author--content'));

        const savedFullAuthorDescriptions = authorDescriptionsElements.map(function (descriptionsElement) {
            return descriptionsElement && descriptionsElement.textContent;
        });

        shortenListTexts(authorDescriptionsElements);
        authorDescriptionsElements.forEach(function each(descriptionElement, index) {
            insertLink(
                descriptionElement,
                savedFullAuthorDescriptions[index],
                ' Lire la suite !',
                'author--content__link',
            )
        });

        function shortenListTexts(descriptionsElements) {
            // descriptionsList should be iterable
            descriptionsElements.forEach(function (descriptionElement) {
                if (!descriptionElement) {
                    return;
                }
                descriptionElement.textContent =
                    shortenPost(descriptionElement.textContent, shortenPostValue(window.innerWidth)) + '…';
            })
        }

        function insertLink(parentNode, parentFullText, linkText, linkClass) {
            const a = document.createElement("a");
            a.classList.add(linkClass);
            a.addEventListener('click', revealFullContent, false);
            a.appendChild(document.createTextNode(linkText));
            parentNode && parentNode.appendChild(a);

            function revealFullContent(e) {
                e.preventDefault();

                if (parentNode) {
                    parentNode.textContent = parentFullText;
                } else {
                    console.error('The authorDescriptionsElements is missing');
                }
            }
        }

        const productDescFull = document.getElementById('content_tab_description');

        if (productDescFull) {


            const moveToTabDesc = function moveToTabDesc(e) {
                e.preventDefault();

                const {top} = tabDesc.getBoundingClientRect();
                doScrolling(top + window.scrollY - 5, 500)
            };


            const tabDesc = document.getElementById('tab_description');


            if (jQuery(document.getElementsByClassName('productShortDesc')).length) {
                const productDescLink = document.getElementsByClassName('productShortDesc');
                productDescLink[0].addEventListener('click', moveToTabDesc, false);
            }
        }

    })();

    (function () {
        function toggleFilters(event) {
            this.nextElementSibling.classList.toggle('filter--hidden');
            this.classList.toggle('filter-title--open');

        }

        const filtersTitles = document.querySelectorAll('[id^="woocommerce_layered_nav-"] .widget-title, [id^="woocommerce_price_filter-"] .widget-title');

        if (filtersTitles) {
            Array.prototype.slice.call(filtersTitles).forEach(function (filtersTitle) {
                filtersTitle.addEventListener('click', toggleFilters, false);
                filtersTitle.classList.add('filter-title');
                filtersTitle.nextElementSibling.classList.add('filter-product');
                filtersTitle.nextElementSibling.classList.add('filter--hidden');
                filtersTitle.nextElementSibling.style.display = 'block'
            });
        }

    })();

    (function () {
        function reorderRoundedImageOnMobile(roundedImage) {
            const imageParent = roundedImage.parentNode.parentNode.parentNode.parentNode;
            imageParent.classList.add('firstImageElement');
            imageParent.parentNode.classList.add('firstImageElement-parent');
        }

        const roundedImage = document.querySelectorAll('.banner-content');

        if (roundedImage) {
            Array.prototype.slice.call(roundedImage).forEach(reorderRoundedImageOnMobile);
        }
    })();

    (function () {
        if (window.matchMedia("(max-width: 1199px)").matches) {
            const menuElems = document.querySelectorAll('#menu-main>[id^="menu-item-"]:not(#menu-item-21565):not(#menu-item-30758), #menu-main>[id^="menu-item-46073"]>.sub-menu>[id^="menu-item-"], #menu-main>[id^="menu-item-46042"]>.sub-menu>[id^="menu-item-"], #menu-main>[id^="menu-item-23780"]>.sub-menu>[id^="menu-item-"]');

            function setAnchor(element) {
                element.firstElementChild.setAttribute("href", "#");
            }

            if (menuElems) {
                Array.prototype.slice.call(menuElems).forEach(setAnchor);
            }

        }
    })();


    // hide the products filters on mobile
    (function () {
        if (window.matchMedia("(max-width: 1199px)").matches) {
            const productFilters = document.querySelector('.sidebar-mobile-bottom .sidebar-left');
            const noveltyButton = document.querySelector('.filter-wrap .woocommerce-ordering');
            const filterContent = document.querySelector('.filter-wrap .filter-content');

            function addElement(element, text, nextElement, parent) {
                const newTag = document.createElement(element);
                const newContent = document.createTextNode(text);
                newTag.appendChild(newContent);
                parent.insertBefore(newTag, nextElement);

                return newTag;
            }

            if (productFilters && noveltyButton && filterContent) {
                productFilters.classList.toggle('u-hide');
                const button = addElement('button', 'Filtrer par', noveltyButton, filterContent);
                button.classList.add('filterButton');
                /*var departement = document.getElementById('woocommerce_layered_nav-4');
                 departement.style.width = '100%';*/
                const sideBarWidgets = document.querySelectorAll('.sidebar-widget');
                Array.prototype.slice.call(sideBarWidgets).forEach(function (widget) {
                    widget.style.float = 'none';
                });
                const filterSubListElems = document.querySelectorAll('.filter-product .wc-layered-nav-term');

                Array.prototype.slice.call(filterSubListElems).forEach(function (listElem) {
                    listElem.style.width = '100%';
                    listElem.style.marginRight = '0';
                    listElem.style.textAlign = 'left';
                });
                // move the filters from the bottom to the top
                filterContent.appendChild(productFilters);

                button.addEventListener('click', function (e) {
                    productFilters.classList.toggle('u-hide');
                }, false)
            }

        }
    })();

    // ajout de parentheses autour des pourcentages de reduction
    // affichage de ces pourcentages
    (function () {
        const promos = document.querySelectorAll('.promo-star');
        Array.prototype.slice.call(promos).forEach(function (promo) {
            promo.textContent = '(' + promo.textContent + ')';
            promo.classList.add('u-show');
        });
    })();
    /*end of zee*/

    (function ($) {
        $(document).ready(function () {
            let objetsWidth = 0;
            let marquesWidth = 0;

            if (window.matchMedia("(min-width: 1199px)").matches) {
                $(".unclickable").on("click", function (e) {

                    const target = $(e.target);
                    const targetParent = target.parent();

                    if (targetParent.hasClass('item-level-1') === true) {
                        return false;
                    }

                });
            }


            if (window.matchMedia("(max-width: 1199px)").matches) {
                const menuElems = document.querySelectorAll('.mobile-nav [id^="menu-item-"]');

                if (menuElems) {
                    Array.from(menuElems).forEach(setAnchor);
                }

                function setAnchor(element) {
                    const children = Array.from(element.children);
                    const childrenClasses = children.map(child => child.className);
                    const isOpenASubCategory = childrenClasses.includes('sub-menu');

                    const [link] = children.filter(child => child.nodeName === 'A');
                    if (link && isOpenASubCategory) {
                        link.setAttribute("href", "#");
                    }
                }
            }

            window.setTimeout(function () {
                $('.cart-bag .ico-sum').css('color', 'white');
            }, 1000);
        });
    })(jQuery);
</script>
</body>
</html>

